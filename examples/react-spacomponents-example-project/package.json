  "scripts": {
    "build": "cross-env NODE_ENV=development npm-run-all build-css build-js",
    "build:production": "cross-env NODE_ENV=production npm-run-all build-css build-js",
    "build-js": "node scripts/build.js && node scripts/build-server.js",
    "build-adobeio": "cross-env NODE_ENV=production node scripts/build-adobeio.js",
    "build-css": "node-sass-chokidar --source-map-embed --include-path ./src --include-path ./node_modules src/ -o src/ --output-style compressed",
    "watch-css": "node-sass-chokidar --source-map-embed --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive --output-style expanded",
    "start-css": "npm-run-all -s build-css watch-css",
    "server": "node dist/server/server.js",
    "install:link-deps": "npm install && npm run link-deps",
    "link-deps": "npm link @adobe/aem-spa-page-model-manager && npm link @adobe/cq-spa-component-mapping && npm link @adobe/aem-react-editable-components",
    "clean": "shx rm -rf ../ui.apps/src/content/jcr_root/apps/core-components-examples/wcm/react/clientlibs/react-spacomponents/resources && shx rm -rf coverage/ dist/ src/tsconfig.types.tsbuildinfo",
    "start-aempack": "node scripts/aempack.js",
    "aempack": "cross-env PROXYHOST=local.aemcomponents.dev NODE_ENV=development npm-run-all -p watch-css start-aempack",
    "aempack-ssr": "cross-env DEVELOPSSR=true  NODE_ENV=development npm-run-all -p watch-css start-aempack",
    "aempack-pub": "cross-env PROXYHOST=local.aemcomponents.dev AEMPORT=4503 cross-env NODE_ENV=development npm-run-all -p watch-css start-aempack",
    "aempack-pub-ssr": "cross-env DEVELOPSSR=true npm-run-all -p aempack-pub"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
