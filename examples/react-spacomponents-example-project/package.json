{
  "name": "@adobe/aem-core-components-react-spa",
  "version": "0.1.0",
  "dependencies": {
    "@adobe/aem-core-components-react-base": "file:../../react-base-components",
    "@adobe/aem-core-components-react-spa": "file:../../react-spa-components",
    "@adobe/aem-react-editable-components": "^1.0.2",
    "@adobe/aem-spa-component-mapping": "^1.0.0",
    "@adobe/aem-spa-page-model-manager": "^1.0.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-universal-component": "^4.0.1"
  },
  "devDependencies": {
    "aempack": "^1.1.9",
    "del-cli": "^3.0.0",
    "@craco/craco": "^5.6.4",
    "craco-alias": "^2.1.1",
    "move-cli": "^1.2.1",
    "cross-env": "^7.0.2",
    "fs-extra": "^9.0.0",
    "node-sass": "^4.13.1",
    "typescript": "4.0.2",
    "@types/react-router-dom": "^5.1.5",
    "mini-css-extract-plugin": "latest"
  },
  "scripts": {
    "build": "npm run clean && craco build && move-cli build ../ui.apps/src/content/jcr_root/apps/core-components-examples/wcm/react/clientlibs/react-spacomponents/resources",
    "build-prod": "npm run clean && cross-env GENERATE_SOURCEMAP=false craco build && move-cli build ../ui.apps/src/content/jcr_root/apps/core-components-examples/wcm/react/clientlibs/react-spacomponents/resources",
    "aempack": "cross-env NODE_ENV=development node scripts/aempack.js",
    "start": "craco start",
    "install:link-deps": "npm install && npm run link-deps",
    "link-deps": "npm link @adobe/aem-spa-page-model-manager && npm link @adobe/aem-spa-component-mapping && npm link @adobe/aem-react-editable-components",
    "clean": "del ../ui.apps/src/content/jcr_root/apps/core-components-examples/wcm/react/clientlibs/react-spacomponents/resources --force && rm -rf coverage/ dist/ src/tsconfig.types.tsbuildinfo",
    "mv": "move-cli build ../ui.apps/src/content/jcr_root/apps/core-components-examples/wcm/react/clientlibs/react-spacomponents/resources",
    "test": "craco test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
